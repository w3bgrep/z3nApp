<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="z3nApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:z3nApp"
    Shell.FlyoutBehavior="Disabled"
    Title="z3nApp">

    <ShellContent
        Title="Main"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Killer"
        ContentTemplate="{DataTemplate local:Killer}"
        Route="Killer" />

    <ShellContent
        Title="Balance"
        ContentTemplate="{DataTemplate local:Balance}"
        Route="Balance" />

    <ShellContent
        Title="WalletTools"
        ContentTemplate="{DataTemplate local:WalletTools}"
        Route="WalletTools" />

    <ShellContent
        Title="DbManager"
        ContentTemplate="{DataTemplate local:DbManager}"
        Route="DbManager" />

    <ShellContent
        Title="ToDo"
        ContentTemplate="{DataTemplate local:ToDo}"
        Route="ToDo" />
</Shell>
